        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "extension": true,
        "outputDir": "jupytercad/labextension",
        "webpackConfig": "./extension.webpack.config.js",
        "sharedPackages": {
            "@jupyter-widgets/base": {
